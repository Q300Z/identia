    "string-strip-html": "^13.4.3",
    "svelte": "^4.2.2",
    "svelte-inview": "^4.0.1",
    "svelte-lite-youtube-embed": "^1.1.0",
    "svelte-tauri-filedrop": "^1.0.0",
    "tauri-plugin-sql-api": "github:tauri-apps/tauri-plugin-sql",
    "timeago.js": "^4.0.2",
    "tslib": "^2.6.2",
