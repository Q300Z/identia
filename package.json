    "@tauri-apps/cli": "^1.5.6",
    "carbon-components-svelte": "^0.81.2",
    "carbon-icons-svelte": "^12.3.0",
    "execa": "^8.0.1",
    "ext2mime": "^2.0.0",
    "flatbuffers": "^23.5.26",
    "get-video-id": "^3.6.5",
    "is-ipfs": "^8.0.1",
    "it-all": "^3.0.3",
    "kubo": "^0.23.0",
